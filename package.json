    "main": "main.js",
    "files": [
        "admin/",
        "build/",
        "python_service/",
        "lib/",
        "install_python.sh",
        "src/",
        "tasks.js",
        "main.js",
        "io-package.json",
        "LICENSE",
        "README.md",
        "README_de.md"
    ],
    "scripts": {
        "prebuild": "rimraf admin/build",
        "build:react": "cd src-admin && npm install && npm run build && cd .. && rimraf admin && xcopy src-admin\\build admin /s /e /y /i",
        "build:backend:dev": "copy src\\main.js main.js && xcopy src\\lib lib /I /Y",
        "build:backend:prod": "javascript-obfuscator src/main.js --output main.js --compact true --self-defending true --disable-console-output false --debug-protection false && javascript-obfuscator src/lib --output lib --compact true --self-defending true --disable-console-output false --debug-protection false",
        "build": "npm run build:react && npm run build:backend:dev",
        "build:prod": "npm run build:react && npm run build:backend:prod",
        "watch": "npm run watch:react",
        "test": "npm run test:js && npm run test:package",
        "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
        "test:package": "mocha test/package --exit"
    },
    "bugs": {
        "url": "https://github.com/obodeldog/ioBroker.cogni-living/issues"
    },
    "readmeFilename": "README.md"
}